    "activityBarBadge.foreground": "#4c4c4c",
    "badge.background": "#ffb700",
    "badge.foreground": "#4c4c4c",
    "banner.background": "#ff0000",
    "banner.foreground": "#ff0000",
    "banner.iconForeground": "#ff0000",
